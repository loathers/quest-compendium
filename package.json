{
  "name": "quest-compendium",
  "version": "0.0.0",
  "description": "quest-compendium is a Typescript library that intends to provide grimoire tasks for automating KoLmafia",
  "repository": "https://github.com/loathers/quest-compedium.git",
  "main": "dist/scripts/quest-compedium/queso.js",
  "author": "Joe Kirchoff <Kirchoff.Joseph.P@gmail.com>",
  "homepage": "",
  "license": "MIT",
  "scripts": {
    "build": "eslint src && node build.mjs",
    "lint": "eslint src && prettier --check .github src *.{cjs,mjs,js,json}",
    "format": "prettier --write .github src *.{cjs,mjs,js,json}",
    "watch": "node build.mjs --watch",
    "install-mafia": "create-kolmafia-script --install"
  },
  "dependencies": {
    "grimoire-kolmafia": "^0.3.26",
    "kolmafia": "^5.28307.0",
    "libram": "^0.9.31"
  },
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.19.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "@types/babel__core": "^7.20.5",
    "@types/babel__preset-env": "^7.9.7",
    "@types/node": "^22.10.10",
    "create-kolmafia-script": "^0.1.0",
    "esbuild": "^0.24.2",
    "esbuild-plugin-babel": "^0.2.3",
    "eslint": "^9.19.0",
    "eslint-plugin-libram": "^0.4.21",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
